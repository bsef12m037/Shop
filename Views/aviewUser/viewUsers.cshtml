@model IEnumerable <MvcProject.Models.newUser>

@{
    ViewBag.Title = "viewUsers";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th>Email Name</th>
        <th>Password</th>
          <th>First Name</th>
          <th>Last Name</th>
         
      </tr>
    </thead>
           <tbody>
        @foreach( var item in Model)
        {
            <tr>
                <td>
                @item.email
                   
                </td>
                <td>
                @item.pwd
                   
                </td>
                  <td>
                
                 @item.fname  
                </td>
                  <td>
                @item.lname
                   
                </td>
                </tr>
           }
               
        
               </tbody>

     </table>
