@model IEnumerable <MvcProject.Models.user>
@{
    ViewBag.Title = "view";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<table class="table table-striped table-bordered table-hover table-condensed">
    <thead>
      <tr>
        <th>User Name</th>
        <th>Password</th>
      </tr>
    </thead>
           <tbody>
        @foreach( var item in Model)
        {
            <tr>
                <td>
                @item.username
                   
                </td>
                <td>
                @item.pwd
                   
                </td>
            </tr>
        }
               </tbody>

     </table>

